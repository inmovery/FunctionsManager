	<DataTemplate x:Key="IconMinimizeTemplate"
				  x:Shared="False">
		<Viewbox Stretch="Uniform">
			<Canvas Width="18"
					Height="1.5">
				<Canvas.RenderTransform>
					<TranslateTransform X="0"
										Y="0" />
				</Canvas.RenderTransform>
				<Rectangle Width="17"
						   Height="1.5"
						   Margin="0,1.5,0,0"
						   RadiusX="0.5"
						   RadiusY="0.5"
						   Fill="#000000" />
			</Canvas>
		</Viewbox>
	</DataTemplate>

	<DataTemplate x:Key="IconMaximizeTemplate"
				  x:Shared="False">
		<Viewbox Stretch="Uniform">
			<Canvas Width="16"
					Height="13">
				<Canvas.RenderTransform>
					<TranslateTransform X="0"
										Y="0" />
				</Canvas.RenderTransform>
				<Rectangle Width="16"
						   Height="13"
						   RadiusX="1.5"
						   RadiusY="1.5"
						   StrokeThickness="1.5"
						   Stroke="#000000" />
			</Canvas>
		</Viewbox>
	</DataTemplate>

