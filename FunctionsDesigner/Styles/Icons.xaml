<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<DataTemplate x:Key="IconMinimizeTemplate"
				  x:Shared="False">
		<Viewbox Stretch="Uniform">
			<Canvas Width="18"
					Height="1.5">
				<Canvas.RenderTransform>
					<TranslateTransform X="0"
										Y="0" />
				</Canvas.RenderTransform>
				<Rectangle Width="17"
						   Height="1.5"
						   Margin="0,1.5,0,0"
						   RadiusX="0.5"
						   RadiusY="0.5"
						   Fill="#000000" />
			</Canvas>
		</Viewbox>
	</DataTemplate>

	<DataTemplate x:Key="IconMaximizeTemplate"
				  x:Shared="False">
		<Viewbox Stretch="Uniform">
			<Canvas Width="16"
					Height="13">
				<Canvas.RenderTransform>
					<TranslateTransform X="0"
										Y="0" />
				</Canvas.RenderTransform>
				<Rectangle Width="16"
						   Height="13"
						   RadiusX="1.5"
						   RadiusY="1.5"
						   StrokeThickness="1.5"
						   Stroke="#000000" />
			</Canvas>
		</Viewbox>
	</DataTemplate>

	<DataTemplate x:Key="IconExitTemplate"
				  x:Shared="False">
		<Viewbox Stretch="Uniform">
			<Canvas Width="18"
					Height="18">
				<Canvas.RenderTransform>
					<TranslateTransform X="0"
										Y="0" />
				</Canvas.RenderTransform>
				<Rectangle Width="24.183"
						   Height="1.27279"
						   RadiusX="0.636395"
						   RadiusY="0.636395"
						   Fill="#000000">
					<Rectangle.RenderTransform>
						<MatrixTransform Matrix="0.707106 0.707108 -0.707106 0.707108 0.900055 9.53674e-07" />
					</Rectangle.RenderTransform>
				</Rectangle>

				<Rectangle Width="24.183"
						   Height="1.27279"
						   RadiusX="0.636395"
						   RadiusY="0.636395"
						   Fill="#000000">
					<Rectangle.RenderTransform>
						<MatrixTransform Matrix="0.707106 -0.707108 0.707106 0.707108 0 17.1" />
					</Rectangle.RenderTransform>
				</Rectangle>
			</Canvas>
		</Viewbox>
	</DataTemplate>

	<DataTemplate x:Key="IconExitFocusTemplate"
				  x:Shared="False">
		<Viewbox Stretch="Uniform">
			<Canvas Width="18"
					Height="18">
				<Canvas.RenderTransform>
					<TranslateTransform X="0"
										Y="0" />
				</Canvas.RenderTransform>
				<Rectangle Width="24.183"
						   Height="1.27279"
						   RadiusX="0.636395"
						   RadiusY="0.636395"
						   Fill="#FFFFFF">
					<Rectangle.RenderTransform>
						<MatrixTransform Matrix="0.707106 0.707108 -0.707106 0.707108 0.900055 0" />
					</Rectangle.RenderTransform>
				</Rectangle>

				<Rectangle Width="24.183"
						   Height="1.27279"
						   RadiusX="0.636395"
						   RadiusY="0.636395"
						   Fill="#FFFFFF">
					<Rectangle.RenderTransform>
						<MatrixTransform Matrix="0.707106 -0.707108 0.707106 0.707108 0 17.1" />
					</Rectangle.RenderTransform>
				</Rectangle>
			</Canvas>
		</Viewbox>
	</DataTemplate>

